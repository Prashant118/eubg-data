  </rdf:Description>
  <rdf:Description rdf:about="http://data.businessgraph.io/ontology#dissolutionYear">
    <ebg:order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</ebg:order>
    <skos:scopeNote>Used for faceting.</skos:scopeNote>
    <skos:definition>Year the organization was dissolved or deregistered, as integer.</skos:definition>
    <rdfs:label>dissolution year</rdfs:label>
